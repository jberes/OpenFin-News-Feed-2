{
    "devtools_port": 9090,
    "startup_app": {
        "name": "OpenFin News Feed 2",
        "description": "News Feed, Crypto, and Stock Prices",
        "uuid": "IGOpenFinNewsFeed2",
        "url": "http://localhost:4205/",
        "applicationIcon": "http://localhost:4205/assets/app.ico",
        "taskbarIcon": "http://localhost:4205/splash.png",
        "minWidth": 700,
        "minHeight": 155,
